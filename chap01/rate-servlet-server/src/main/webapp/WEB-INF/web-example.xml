<web-app xmlns="https://jakarta.ee/xml/ns/jakartaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee
                      https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd"
  version="6.0"
  metadata-complete="true">

  <!--
    web.xml 로서 사용 가능하게 두면
    rateServlet의 서블릿 매핑설정을 제거하고 실행해도
    RateServlet에 붙여둔 @WebServlet(urlPatterns = "/calc") 가 동작을 안한다.

    그래서 참고를 위해 삭제는 하지 않고, 파일명만 web-example.xml로 둠.
  -->


  <display-name>RateServer</display-name>
  <!--
  <servlet>
    <servlet-name>rateServlet</servlet-name>
    <servlet-class>org.mklinkj.taojwp.sevlet.RateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>rateServlet</servlet-name>
    <url-pattern>/calc</url-pattern>
  </servlet-mapping>
  -->

  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.xhtml</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>


</web-app>
